#:kivy 2.0.0
#:include ui/skyview.kv

<DarkSky>:
    canvas:
        Color:
            rgba: .2, .2, .2, 1
        Rectangle:
            pos: (0, 0)
            size: self.size

    FloatLayout:
        size: root.size

        SkyView:
            pos_hint: {'center_x:': 0.5, 'center_y': 0.8}
            size_hint: (0.1, 0.1)